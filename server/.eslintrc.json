{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": "airbnb",
  "plugins": [],
  "rules": {
    "comma-dangle": 0,
    "import/newline-after-import": 0,
    "react/jsx-filename-extension": 0,
    "arrow-parens": 0,
    "consistent-return": 0,
    "object-curly-newline": 1,
    "global-require": 1
  }
}
